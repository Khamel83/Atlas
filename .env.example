
# Atlas Configuration
# Copy this file to .env and fill in the values.

# --- Core Configuration ---
# The root directory for all Atlas data.
DATA_DIRECTORY=output

# --- API Keys ---
# OpenRouter API key for AI features (optional)
OPENROUTER_API_KEY=

# --- Transcription ---
# Enable or disable transcription services.
TRANSCRIBE_ENABLED=false

# --- Instapaper ---
# Instapaper API credentials (optional)
INSTAPAPER_API_KEY=
INSTAPAPER_API_SECRET=
INSTAPAPER_USERNAME=
INSTAPAPER_PASSWORD=
INSTAPAPER_CONSUMER_KEY=
INSTAPAPER_CONSUMER_SECRET=
