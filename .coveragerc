[run]
source = helpers, ask, web, scripts
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    .venv/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

ignore_errors = True
show_missing = True
skip_covered = False

[html]
directory = htmlcov
title = Atlas Test Coverage Report

[xml]
output = coverage.xml